  "installCommand": "pnpm install",
  "framework": "nextjs",
  "outputDirectory": "apps/frontend/.next",
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "https://your-railway-backend-url.up.railway.app"
  },
